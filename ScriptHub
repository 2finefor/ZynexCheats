local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "ZYNEX Hub",
   LoadingTitle = "Universal Game Hub",
   LoadingSubtitle = "by Zynex",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Zynex Hub",
      Subtitle = "Key System",
      Note = "Join discord for the ",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Zynex"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local ScriptTab = Window:CreateTab("Scripts", nil) -- Title, Image
local Section = ScriptTab:CreateSection("Blade Ball")


local Button = ScriptTab:CreateButton({
   Name = "Op Blade Ball Sript",
   Callback = function()
   loadstring(game:HttpGet("https://pastefy.app/Mmn9x7Vn/raw"))()
   end,
})

local Section = ScriptTab:CreateSection("Phantom Forces")

local Button = ScriptTab:CreateButton({
   Name = "Quantom Cheats",
   Callback = function()
           loadstring(game:HttpGet('https://raw.githubusercontent.com/2finefor/ZynexCheats/main/beta'))()
   end,
})

